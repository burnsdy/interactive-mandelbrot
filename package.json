{
  "name": "mandelbrot-explorer",
  "version": "0.1.0",
  "description": "A web application to interactively explore the Mandelbrot set fractal",
  "author": "Dylan Burns <burnsdy@gmail.com>",
  "license": "MIT",
  "keywords": [
    "mandelbrot",
    "fractal"
  ],
  "lint-staged": {
    "*.js": "eslint --cache",
    "*.{js,css,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "echo \"Successful commit âœ…\"",
      "pre-push": "yarn && yarn test && yarn lint"
    }
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "format": "prettier --write --list-different \".\"",
    "lint": "next lint",
    "start": "next start",
    "start:dev": "next dev",
    "start:prod": "next build && next start",
    "test": "jest --passWithNoTests",
    "test:coverage": "rm -rf coverage && yarn test --coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@next/bundle-analyzer": "^13.1.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/js-cookie": "^3.0.2",
    "@types/leaflet": "^1.9.0",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-katex": "^3.0.0",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^4.3.8",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.3",
    "@mantine/hooks": "^5.10.3",
    "@mantine/next": "^5.10.3",
    "@react-leaflet/core": "^2.1.0",
    "@reduxjs/toolkit": "^1.9.2",
    "@tabler/icons-react": "^2.2.0",
    "@wasm-tool/wasm-pack-plugin": "^1.5.0",
    "js-cookie": "^3.0.1",
    "leaflet": "^1.9.3",
    "next": "^13.4.19",
    "next-redux-wrapper": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-katex": "^3.0.1",
    "react-leaflet": "^4.2.0",
    "react-redux": "^8.0.5",
    "threads": "^1.7.0"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
